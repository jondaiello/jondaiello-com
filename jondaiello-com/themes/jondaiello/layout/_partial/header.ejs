<header>
  <h1><a href="<%- url_for() %>" id="logo"><%= config.title %></a></h1>
  <% if (theme.subtitle){ %>
    <strong><a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle %></a></strong>
  <% } %>

  <nav>
    <% for (var i in theme.menu){ %>
      <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
    <% } %>
  </nav>
</header>
